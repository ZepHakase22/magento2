<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="page.main.title" destination="content" before="category.products"/>
        <!--<referenceBlock name="page.main.title" remove="true" />-->
        <move element="category.image" destination="content" after="page.main.title"/>
        <move element="category.description" destination="content" after="category.image"/>
        <move element="category.cms" destination="content" after="category.description"/>
    </body>
</page>
